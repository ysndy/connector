<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.connector">

    <uses-permission android:name="android.permission.CALL_PHONE"/>


    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".doyeon.activity.SupplierProfileActivity"/>
        <activity android:name=".doyeon.activity.TransactionProductsActivity"/>
        <activity android:name=".doyeon.activity.SearchActivity"/>
        <activity android:name=".doyeon.activity.ProfileListActivity"/>
        <activity android:name=".doyeon.activity.TransactionCalendarActivity"/>
        <activity android:name=".doyeon.activity.MainPageActivity" android:launchMode="singleTop"/>
        <activity android:name=".jeongeun.Provider_mypage"/>
        <activity android:name=".soohyun.MyPage"/>
        <activity android:name=".soohyun.Login"/>
        <activity android:name=".soohyun.Join"/>
    </application>

</manifest>